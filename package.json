{
  "name": "template-react",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start:dev": "env-cmd -f .env.development next dev --turbopack",
    "start:prod": "env-cmd -f .env.production next dev --turbopack",
    "start:local": "env-cmd -f .env.local next dev --turbopack",
    "build:dev": "env-cmd -f .env.development next build",
    "build:prod": "env-cmd -f .env.production next build",
    "build:local": "env-cmd -f .env.local next build",
    "host:dev": "yarn build:dev && env-cmd -f .env.development next start",
    "host:prod": "yarn build:prod && env-cmd -f .env.production next start",
    "host:local": "yarn build:local && env-cmd -f .env.local next start",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "env-cmd": "^11.0.0",
    "next": "16.0.7",
    "postbuild": "^2.1.0",
    "react": "19.2.1",
    "react-dom": "19.2.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "19.2.1",
    "@types/react-dom": "19.2.1",
    "eslint": "^9",
    "eslint-config-next": "16.0.7",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
